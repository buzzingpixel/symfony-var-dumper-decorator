{
    "name": "mission-control-cli",
    "description": "Mission Control Developer CLI",
    "author": "TJ Draper",
    "bin": {
        "cli": "./bin/run"
    },
    "license": "UNLICENSED",
    "private": true,
    "main": "dist/index.js",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@oclif/core": "^1",
        "@oclif/plugin-commands": "^2.2.0",
        "@oclif/plugin-help": "^5",
        "chalk": "^4.1.2",
        "fs-extra": "^11.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/eslint-parser": "^7.18.9",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.7.6",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "globby": "^11",
        "oclif": "^3",
        "shx": "^0.3.3",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.9.5"
    },
    "oclif": {
        "bin": "./dev",
        "dirname": "cli",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-commands"
        ],
        "topicSeparator": " ",
        "topics": {}
    },
    "scripts": {
        "build": "rm -rf dist && yarn tsc -b"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "types": "dist/index.d.ts"
}
